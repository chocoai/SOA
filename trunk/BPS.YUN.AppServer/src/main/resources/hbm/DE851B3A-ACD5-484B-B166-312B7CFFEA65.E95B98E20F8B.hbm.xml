<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "PUBLIC" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class entity-name="AMS_BonusDate" table="DYNA_AMS_BonusDate">
    <id name="id" type="long">
      <generator class="sequence">
        <param name="sequence">IDSEQ_AMS_BonusDate</param>
      </generator>
    </id>
    <property name="domainId" not-null="false" type="long" index="IDX_DOMAINID"/>
    <property name="member_id" not-null="false" type="long"/>
    <property name="bonus_rule" not-null="false" type="string" length="100"/>
    <property name="day_num" not-null="false" type="long"/>
    <property name="day_total" not-null="false" type="long"/>
    <property name="belong_time" not-null="false" type="timestamp" length="100"/>
    <property name="member_userId" not-null="false" type="string" length="100"/>
    <property name="member_phone" not-null="false" type="string" length="100"/>
  </class>
</hibernate-mapping>
