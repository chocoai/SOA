<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "PUBLIC" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class entity-name="AMS_Bank" table="DYNA_AMS_Bank">
    <id name="id" type="long">
      <generator class="sequence">
        <param name="sequence">IDSEQ_AMS_Bank</param>
      </generator>
    </id>
    <property name="domainId" not-null="false" type="long" index="IDX_DOMAINID"/>
    <property name="name" not-null="false" type="string" length="100"/>
    <property name="bank_code" not-null="false" type="string" length="100"/>
    <property name="short_name" not-null="false" type="string" length="100"/>
    <property name="its_bank_code" not-null="false" type="string" length="100"/>
    <property name="logo" not-null="false" type="string" length="200"/>
    <property name="en_bank_code" not-null="false" type="string" length="100"/>
  </class>
</hibernate-mapping>
