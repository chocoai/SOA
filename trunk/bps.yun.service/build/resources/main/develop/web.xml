<?xml version="1.0" encoding="utf-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
    <display-name>IMEServer</display-name>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>default.html</welcome-file>
        <welcome-file>default.htm</welcome-file>
        <welcome-file>default.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <display-name>StartupServlet</display-name>
        <servlet-name>StartupServlet</servlet-name>
        <servlet-class>apf.framework.StartupServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <display-name>InitServlet</display-name>
        <servlet-name>InitServlet</servlet-name>
        <servlet-class>ime.servlet.InitServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <display-name>Export</display-name>
        <servlet-name>Export</servlet-name>
        <servlet-class>ime.servlet.ExportServlet</servlet-class>
    </servlet>
    <servlet>
        <display-name>UploadFile</display-name>
        <servlet-name>UploadFile</servlet-name>
        <servlet-class>ime.servlet.UploadFile</servlet-class>
    </servlet>
    <servlet>
        <display-name>QueryServlet</display-name>
        <servlet-name>QueryServlet</servlet-name>
        <servlet-class>ime.servlet.QueryServlet</servlet-class>
    </servlet>
    <servlet>
        <display-name>SOAServlet</display-name>
        <servlet-name>SOAServlet</servlet-name>
        <servlet-class>ime.soa.SOAServlet</servlet-class>
    </servlet>
    <servlet>
        <description></description>
        <display-name>JHTMLServlet</display-name>
        <servlet-name>JHTMLServlet</servlet-name>
        <servlet-class>ime.servlet.JHTMLServlet</servlet-class>
    </servlet>
    
    <servlet>
        <description></description>
        <display-name>dwr-invoker</display-name>
        <servlet-name>dwr-invoker</servlet-name>
        <servlet-class>org.directwebremoting.servlet.DwrServlet</servlet-class>
        <init-param>
            <param-name>org.directwebremoting.extend.Remoter</param-name>
            <param-value>ime.dwr.TransactionRemoter</param-value>
        </init-param>
		<init-param>
            <param-name>maxCallCount</param-name>
            <param-value>100</param-value>
        </init-param>
        <init-param>
			<param-name>allowGetForSafariButMakeForgeryEasier</param-name>
			<param-value>true</param-value>
		</init-param>
		<init-param>
			<param-name>crossDomainSessionSecurity</param-name>
			<param-value>false</param-value>
		</init-param>
		<init-param>
			<param-name>allowScriptTagRemoting</param-name>
			<param-value>true</param-value>
		</init-param>
        <init-param>
		    <param-name>classes</param-name>
		    <param-value>ime.tools.RemoteTransaction</param-value>
        </init-param>
        <init-param>
            <param-name>config</param-name>
            <param-value>/WEB-INF/dwr.ime.core.xml</param-value>
        </init-param>
        <init-param>
            <param-name>config-organization</param-name>
            <param-value>/WEB-INF/dwr.ime.organization.xml</param-value>
        </init-param>
        <init-param>
            <param-name>config-workflow</param-name>
            <param-value>/WEB-INF/dwr.ime.workflow.xml</param-value>
        </init-param>
        <init-param>
            <param-name>config-calendar</param-name>
            <param-value>/WEB-INF/dwr.ime.calendar.xml</param-value>
        </init-param>
        <init-param>
            <param-name>config-message</param-name>
            <param-value>/WEB-INF/dwr.ime.message.xml</param-value>
        </init-param>
        <init-param>
            <param-name>config-bizservice</param-name>
            <param-value>/WEB-INF/dwr.ime.bizservice.xml</param-value>
        </init-param>
        <init-param>
            <param-name>config-webcallcenter</param-name>
            <param-value>/WEB-INF/dwr.ime.webcallcenter.xml</param-value>
        </init-param>
    </servlet>
    <servlet>
		<description></description>
		<display-name>dwrProxy-invoker</display-name>
		<servlet-name>dwrProxy-invoker</servlet-name>
		<servlet-class>ime.dwr.DwrProxyServlet</servlet-class>
		<init-param>
			<param-name>xxx</param-name>
			<param-value>http://localhost:8086/admin/dwr</param-value>
		</init-param>
	</servlet>
	
    <!-- MessageBroker Servlet -->
    <servlet>
        <servlet-name>MessageBrokerServlet</servlet-name>
        <servlet-class>flex.messaging.MessageBrokerServlet</servlet-class>
        <init-param>
            <param-name>services.configuration.file</param-name>
            <param-value>/WEB-INF/flex/services-config.xml</param-value>
       </init-param>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>XPTServlet</servlet-name>
        <servlet-class>ime.web.xpt.XPTServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
	<servlet>
        <servlet-name>MobileUIServlet</servlet-name>
        <servlet-class>ime.mobile.MobileUIServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>MessageBrokerServlet</servlet-name>
        <url-pattern>/messagebroker/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dwr-invoker</servlet-name>
        <url-pattern>/dwr/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
		<servlet-name>dwrProxy-invoker</servlet-name>
		<url-pattern>/dwrProxy/*</url-pattern>
	</servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadFile</servlet-name>
        <url-pattern>/svr/UploadFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Export</servlet-name>
        <url-pattern>/svr/export</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QueryServlet</servlet-name>
        <url-pattern>/svr/query</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SOAServlet</servlet-name>
        <url-pattern>/svr/soa</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JHTMLServlet</servlet-name>
        <url-pattern>*.jhtml</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>XPTServlet</servlet-name>
        <url-pattern>*.xpt</url-pattern>
    </servlet-mapping>
	<servlet-mapping>
        <servlet-name>MobileUIServlet</servlet-name>
        <url-pattern>*.mxpt</url-pattern>
    </servlet-mapping>
    
	<servlet> 
		<servlet-name>webdav</servlet-name> 
		<servlet-class>ime.teamwork.webdav.WebdavServlet</servlet-class> 
	</servlet>
	<servlet-mapping>
		<servlet-name>webdav</servlet-name>
		<url-pattern>/svr/fs/*</url-pattern>
	</servlet-mapping>

    <!-- app servlet-->
	<servlet>
        <display-name>ApplicationServlet</display-name>
        <servlet-name>ApplicationServlet</servlet-name>
        <servlet-class>ime.servlet.ApplicationServlet</servlet-class>
    </servlet>
	<servlet-mapping>
        <servlet-name>ApplicationServlet</servlet-name>
        <url-pattern>/svr/app</url-pattern>
    </servlet-mapping>
    
    <!-- xmpp server 尽适用于Apache Tomcat 7.0.27 以上版本 -->
    <!-- 
    <servlet>
	  <servlet-name>WebSocket Servlet</servlet-name>
	  <servlet-class>ime.xmpp.vysper.XmppWebSocketServlet</servlet-class>
	  <load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
	  <servlet-name>WebSocket Servlet</servlet-name>
	  <url-pattern>/xmpp</url-pattern>
	</servlet-mapping>
	-->
	 
	<!-- 实现XBAP自动更新的代理网关 -->
    <servlet> 
		<servlet-name>HttpProxyServler</servlet-name> 
		<servlet-class>ime.servlet.HttpProxyServlet</servlet-class> 
		<init-param>
			<param-name>prefix</param-name>
			<param-value>/svr/xbap/</param-value>
		</init-param>
		<init-param>
			<param-name>target</param-name>
			<param-value>http://www.91quick.com/publish/QWebDesktop/</param-value>
		</init-param>
		<init-param>
			<param-name>BackupPath</param-name>
			<param-value>/downloads/xbap/</param-value>
		</init-param>
		<init-param>
			<param-name>timeout</param-name>
			<param-value>2000</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>HttpProxyServler</servlet-name>
		<url-pattern>/svr/xbap/*</url-pattern>
	</servlet-mapping>
	
	<mime-mapping>
        <extension>xbap</extension>
        <mime-type>application/x-ms-xbap</mime-type>
    </mime-mapping>
	<mime-mapping>
        <extension>manifest</extension>
        <mime-type>application/manifest</mime-type>
    </mime-mapping>
	<mime-mapping>
        <extension>xaml</extension>
        <mime-type>application/xaml+xml</mime-type>
    </mime-mapping>
	<mime-mapping>
        <extension>application</extension>
        <mime-type>application/x-ms-application</mime-type>
    </mime-mapping>
	<mime-mapping>
        <extension>deploy</extension>
        <mime-type>application/octet-stream</mime-type>
    </mime-mapping>
	<mime-mapping>
        <extension>xps</extension>
        <mime-type>application/vnd.ms-xpsdocument</mime-type>
    </mime-mapping>

	<!--
	<listener>
		<listener-class>ime.server.ServerMonitor</listener-class>
		<listener-class>flex.messaging.HttpFlexSession</listener-class>
	</listener>
	-->
	<listener>
		<listener-class>ime.server.ServerMonitor</listener-class>
	</listener>
	<listener>
		<listener-class>flex.messaging.HttpFlexSession</listener-class>
	</listener>

	<filter>
        <filter-name>Set Character Encoding</filter-name>
        <filter-class>filters.SetCharacterEncodingFilter</filter-class>
        <init-param>
            <param-name>encoding</param-name>
            <param-value>UTF-8</param-value>
        </init-param>
    </filter>
    <filter>
        <filter-name>Clean Memory</filter-name>
        <filter-class>filters.CleanMemoryFilter</filter-class>
    </filter>
    
    <filter-mapping>
        <filter-name>Set Character Encoding</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>Clean Memory</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
</web-app>
