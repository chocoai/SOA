	button_img_ckd: ws_app_path + "images/share/ws/closed.gif";
	button_img_uku: ws_app_path + "images/share/ws/expend.gif";
	button_img_uko: ws_app_path + "images/share/ws/expend.gif";
	button_img_ukd: ws_app_path + "images/share/ws/expend.gif";
	}

